{
  "name": "react-toolbox",
  "description": "A set of React components implementing Google's Material Design specification with the power of CSS Modules.",
  "homepage": "http://www.react-toolbox.com",
  "version": "1.3.4",
  "author": {
    "name": "React Toolbox Team",
    "url": "https://github.com/react-toolbox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-toolbox/react-toolbox.git"
  },
  "keywords": [
    "components",
    "material design",
    "react",
    "react-component",
    "toolkit"
  ],
  "license": "MIT",
  "filename": "index.min.js",
  "npmName": "react-toolbox",
  "npmFileMap": [
    {
      "basePath": "lib",
      "files": [
        "+(index|identifiers)*.+(js|css|map)",
        "**/!(__test__|__tests__)/*.+(css|js|map)"
      ]
    }
  ]
}
